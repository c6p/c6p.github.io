/*.jpg
  Cache-Control: public, max-age=60, immutable
/*.png
  Cache-Control: public, max-age=60, immutable
/*.webp
  Cache-Control: public, max-age=60, immutable
/*.svg
  Cache-Control: public, max-age=60, immutable
/*.js
  Cache-Control: public, max-age=60, immutable
/*.css
  Cache-Control: public, max-age=60, immutable
/*
  Cache-Control: public, max-age=300
  Clear-Site-Data: "*"
  Strict-Transport-Security: max-age=86400; includeSubDomains; preload
  X-Content-Type-Options: nosniff
  X-XSS-Protection: 1; mode=block
  Content-Security-Policy: default-src 'none'; base-uri 'self'; manifest-src 'self'; connect-src 'self'; font-src 'self' https://cdn.jsdelivr.net; img-src 'self' data:; script-src {{readFile "data/inline_script_hash.txt"}} 'self' {{readFile "data/external_script_source.txt"}}; style-src 'self' 'unsafe-inline' https://cdn.jsdelivr.net; object-src 'none'
  X-Frame-Options: DENY
  Referrer-Policy: strict-origin
  Permission-Policy: 'self'